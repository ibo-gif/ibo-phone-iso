# IBO Phone Kernel Configuration
# Конфигурация Linux kernel для IBO Phone

# Версия kernel
CONFIG_VERSION=5.15.0

# Архитектура
CONFIG_ARM64=y
CONFIG_ARM64_4K_PAGES=y
CONFIG_ARM_SMCCC=y

# Файловые системы
CONFIG_EXT4_FS=y
CONFIG_EXT4_FS_SECURITY=y
CONFIG_TMPFS=y
CONFIG_TMPFS_POSIX_ACL=y
CONFIG_SQUASHFS=y

# Сеть
CONFIG_NET=y
CONFIG_INET=y
CONFIG_IPV4=y
CONFIG_IPV6=y
CONFIG_WIRELESS=y
CONFIG_MAC80211=y
CONFIG_CFG80211=y

# USB поддержка
CONFIG_USB=y
CONFIG_USB_GADGET=y
CONFIG_USB_ANDROID=y
CONFIG_USB_ADB=y

# Мультимедиа
CONFIG_MEDIA_SUPPORT=y
CONFIG_VIDEO_CAPTURE_SUPPORT=y
CONFIG_AUDIO_SUPPORT=y

# Сенсоры
CONFIG_INPUT=y
CONFIG_TOUCHSCREEN=y
CONFIG_TOUCHSCREEN_SYNAPTICS=y

# Батарея и питание
CONFIG_POWER_SUPPLY=y
CONFIG_BATTERY_BQ27X00=y

# Дисплей
CONFIG_DRM=y
CONFIG_DRM_PANEL=y
CONFIG_BACKLIGHT_LCD_SUPPORT=y
CONFIG_LCD_CLASS_DEVICE=y

# Безопасность
CONFIG_SECURITY=y
CONFIG_SECURITY_SELINUX=y
CONFIG_SECURITY_SELINUX_ENFORCE=y
CONFIG_CRYPTO_SHA256=y
CONFIG_CRYPTO_AES=y

# Отладка
CONFIG_DEBUG_INFO=y
CONFIG_DYNAMIC_DEBUG=y
CONFIG_TRACEPOINTS=y

# Оптимизация производительности
CONFIG_SMP=y
CONFIG_HAVE_SMP=y
CONFIG_NR_CPUS=4
CONFIG_HZ_1000=y
